        android:layout_height="wrap_content"
        android:text="Search Items"
        android:textAlignment="center"
        android:textColor="@color/alm_black"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />
    <androidx.cardview.widget.CardView
        android:id="@+id/music_card1"
        android:layout_width="150dp"
        android:layout_height="135dp"
        android:layout_gravity="center"
        android:clickable="true"
        android:elevation="2dp"
        android:foreground="?android:attr/selectableItemBackground"
        app:cardCornerRadius="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/thumbnail_music1"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:contentDescription="blank"
                android:src="@drawable/test"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/music_text1"
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:text=""
                android:textAlignment="center"
                android:textColor="@color/alm_black"
                android:textSize="12sp"
                app:layout_constraintTop_toBottomOf="@+id/thumbnail_music1" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

